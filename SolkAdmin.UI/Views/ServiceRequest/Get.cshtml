        <td>
            @Html.DisplayFor(modelItem => item.TimeOccured)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VehicleNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClaimType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UsageType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegistrationCategory)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VehicleValue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VehicleYear)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsFinanced)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BuyerResponded)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Location)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClientType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
