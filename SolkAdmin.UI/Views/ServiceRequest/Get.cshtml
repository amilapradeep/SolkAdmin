@model IEnumerable<SolkAdmin.DTO.ServiceRequest>

@{
    ViewBag.Title = "ServiceRequest";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ServiceRequest</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InsuranceTypeId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeOccured)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VehicleNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClaimType)
